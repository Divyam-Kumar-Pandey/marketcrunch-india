generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String?
  creditsUsed Int      @default(0)
  creditsAvailable Int  @default(10)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
